import {RouterModule, Routes} from '@angular/router';
import {NgModule} from '@angular/core';
import {FirmwareListComponent} from './firmware-list/firmware-list.component';
import {FirmwareInfoComponent} from './firmware-info/firmware-info.component';
import {FirmwareComponent} from './firmware.component';

const firmwareRoutes: Routes = [
  { path: '', component: FirmwareComponent, children: [
      {path: '', component: FirmwareListComponent},
      {path: ':id', component: FirmwareInfoComponent}
    ]
  },
];

@NgModule({
  imports: [RouterModule.forChild(firmwareRoutes)],
  exports: [RouterModule]
})
export class FirmwareRoutingModule { }
