export class DeviceCreateFields {
    CreateFields?: CreateField[];
}

export class CreateField {
    Name?: string;
    Description?: string;
    FieldType?: string; // Text, Dropdown, Number- used to correctly display create ui
    Enable?: boolean;
    DefaultValue?: any;
    OptionValues?: any;
    Required?: boolean;
    MaxLength?: number; // will be used as maximum length if text, maximum number if integer values
    MinLength?: number; // will be used as minimum length if text, minimum number on integer values
    FileExtension?: string;
    AlphaNumeric?: boolean; // true for alphanumeric, false to allow special characters.
                            // Special characters allowed: dash and underscore only.
}
export class CreateFieldResults {
    CreateFieldResults?: CreateFieldResult[];
}

export class CreateFieldResult {
    Name?: string;
    Value?: any;
}
