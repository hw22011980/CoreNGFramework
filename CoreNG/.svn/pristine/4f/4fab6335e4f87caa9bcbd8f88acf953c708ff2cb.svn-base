<ng-container *ngIf="configs">
  <tr *ngIf="store.raw_data.length==0">
    <td [innerHTML]="configs.data_loading_text" [attr.colspan]="columns.length + 1" style="text-align: center"></td>
  </tr>
  <tr app-filter-row [columns]="columns" [configs]="configs" [store]="store" [internal_configs]="internal_configs"
    *ngIf="configs.filter" [expand_tracker]="expand_tracker" [ngClass]="configs.css.row_filter_class">
  </tr>
  <ng-container *ngIf="configs.subgrid">
    <tr app-subgrid *ngFor="let row_data of display_data" class="subgrid-row" [configs]="configs"
      [internal_configs]="internal_configs" [expand_tracker]="expand_tracker" [store]="store" [row_data]="row_data"
      [cellclick]="cellclick" [rowselect]="rowselect" [rowdeselect]="rowdeselect" [expand]="expand">
    </tr>
  </ng-container>
  <ng-container *ngIf="!configs.subgrid">
    <tr *ngFor="let row_data of display_data" [hidden]="row_data.hidden"
      [attr.class]="configs.row_class_function(row_data) + ' ' + (row_data.row_selected ? configs.css.row_selection_class : '')"
      (click)="selectRow(row_data, $event)">
      <ng-container *ngIf="expand_tracker[row_data.parent_pathx]">
        <td *ngIf="configs.multi_select" class="checkbox_column">
          <input type="checkbox" [checked]="row_data.row_selected" (click)="selectRowOnCheck(row_data, $event)"
            [attr.disabled]="row_data.selection_disabled">
        </td>
        <td *ngFor="let column of columns; index as i" class="{{column.css_class}}"
          [ngClass]="{'column-hide': column.hidden, 'expand-column': i == 0}">
          <app-tree-cell [configs]="configs" [column]="column" [index]="i" [row_data]="row_data"
            [expand_tracker]="expand_tracker" [cellclick]="cellclick" (rowexpand)="onRowExpand($event)"
            (rowcollapse)="onRowCollapse($event)">
          </app-tree-cell>
        </td>
        <td app-tree-cell-actions *ngIf="configs.actions.view_compare" class="compare_column" [row_data]="row_data"
          [configs]="configs" [store]="store">
        </td>
        <ng-container *ngIf="configs.compare_select">
          <td class="radio_column">
            <input type="radio" name="{{row_data.id}}" class="radio-btn" [checked]="row_data.rdo1_selected" (click)="selectRowOnRdo1(row_data, $event)"
              [disabled]="row_data.selection_disabled">
            <img src={{configs.compare_select_one_imgUrl}} class="img-container">
          </td>
          <td class="radio_column">
            <input type="radio" name="{{row_data.id}}" class="radio-btn" [checked]="row_data.rdo2_selected" (click)="selectRowOnRdo2(row_data, $event)"
              [disabled]="row_data.selection_disabled">
            <img src={{configs.compare_select_two_imgUrl}} class="img-container">
          </td>
        </ng-container>
      </ng-container>
    </tr>
    <tr *ngIf="configs.show_summary_row">
      <td *ngIf="configs.multi_select"></td>
      <td *ngFor="let column of configs.columns">
        <div [innerHTML]="column.summary_renderer && column.summary_renderer(display_data)"></div>
      </td>
    </tr>
  </ng-container>

</ng-container>