<div class="card">
  <div>
    <button data-dismiss="alert" data-target="#closeablecard" id="close-button" type="button" class="close"
      (click)="close()" aria-label="Close">
      <span aria-hidden="true">&times;</span>
    </button>
  </div>
  <div class="card-body">
    <form novalidate [formGroup]="formImport">
      <div class="input-group mb-3">
        <div class="custom-file">
          <input type="file" class="custom-file-input" multiple formControlName="importFile" id="importFile"
            (change)="onFileChange($event.target.files)">
          <label class="custom-file-label" #labelImport for="inputXMLFile"><i class="fas fa-search"></i> Choose xml
            file...</label>
        </div>
      </div>
      <div class="form-group">
        <button class="btn btn-outline-secondary btn-sm" type="button" (click)="import()"><i
            class="fas fa-file-import"></i> Import
        </button>
      </div>
    </form>
  </div>
</div>