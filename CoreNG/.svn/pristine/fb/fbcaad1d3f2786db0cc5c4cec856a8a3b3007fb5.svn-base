<div class="card mb-5 rounded w-100" *ngIf="true">
  <div class="card-header">Daftar Menu</div>
  <a href="#">
    <span class="glyphicon glyphicon-cog"></span>
  </a>
  <div id="setup-content" class="container-fluid">
    <div class="row">      
      <div class="col-4 overflow-auto" id="column-pills-tab" >         
         <app-nav-tree #navtree *ngIf="menus" [data]="menus" [disableClick]="!disableClick" (onClick)="clickMenu($event)"></app-nav-tree>
      </div> 
      <div class="col-8">
        <div class= "mt-5">
          <!--<app-form-entry [frmconfig]="frmconfig" ></app-form-entry>-->
        </div>
      </div>    
    </div>
  </div>
</div>
