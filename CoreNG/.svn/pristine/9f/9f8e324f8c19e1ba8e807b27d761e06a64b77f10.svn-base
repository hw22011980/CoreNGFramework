export class DeviceType {
  public deviceTypeId: number;
  public deviceTypeName: string;
  public availableModels: any;

  constructor(DeviceTypeId: number, DeviceTypeName: string, avmodels: any) {
    this.deviceTypeId = DeviceTypeId;
    this.deviceTypeName = DeviceTypeName;
    this.availableModels = avmodels;
  }

}

export class DeviceModel {
  public deviceModelID: number;
  public deviceModelName: string;


  constructor(deviceModelID: number, deviceModelName: string) {
    this.deviceModelID = deviceModelID;
    this.deviceModelName = deviceModelName;
  }

}

export class DeviceSite {
  public siteID: number;
  public siteName: string;


  constructor(siteID: number, siteName: string) {
    this.siteID = siteID;
    this.siteName = siteName;
  }

}
