tr {
    background:#700c0e;
}
tr th {
    font-size: 1.2em;
    font-weight: bold;
    color: #ffffff;
    border: 1px solid #cdd5dc;
    vertical-align: middle;
    position: relative;
    box-sizing: border-box;
}
tr th.column-hide {
    display: none;
}
tr th.action-column{
    text-align: center;
}
tr th.action-column span.icon-container {
    cursor: pointer;
}
tr th i.plus {
    border: 1px solid rgba(0, 0, 0, 0);
}
tr th i.plus:before {
    content: '';
    width: 18px;
    height: 1px;
    border-top: 2px solid #222;
    display: block;
    position: absolute;
    margin-top: -15px;
    margin-left: 6px;
}
tr th i.plus:after {
    content: '';
    width: 1px;
    height: 18px;
    border-right: 2px solid #222;
    display: block;
    position: absolute;
    margin-top: -23px;
    margin-left: 14px;
}
th.clear-left-border {
    border-left: 0 !important;
}
th.clear-right-border {
    border-right: 0 !important;
}
.column-hide {
    display: none;
}
svg {
    width: 25px;
    padding-right: 3px;
}
.radio_column{
    text-align: center;
}

