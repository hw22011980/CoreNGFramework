<div class="container margin-left-info pt-5">
  <nav aria-label="breadcrumb">
    <ol class="breadcrumb w-75">
      <li class="breadcrumb-item"><a class="header" [routerLink]="['/firmwares']">Firmwares</a></li>
      <li *ngIf='firmware!=null' class="breadcrumb-item active" aria-current="page">{{firmware.fwCode}}</li>
    </ol>
  </nav>
  <div class="card mb-5 bg-white rounded w-75" *ngIf='firmware!=null'>
    <app-info-display (myEvent)="onUpdateInformation($event)" [informationFields]="infoModel"></app-info-display>
  </div>
</div>