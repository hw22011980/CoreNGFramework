export class DeviceConnectStatus {
  public plantNumber: string;
  public isConnected = false;
  public isChecked = false;
  public deviceId : number;

  public CheckConnectionStatus(): boolean {
    if (localStorage.hasOwnProperty('previousId')) {
      const connectstatus: DeviceConnectStatus = JSON.parse(localStorage.getItem('previousId'));
      return connectstatus.isConnected;
    } else {
      return false;
    }
  }
}
