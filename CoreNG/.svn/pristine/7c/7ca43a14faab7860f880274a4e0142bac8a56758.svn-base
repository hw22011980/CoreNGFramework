import { RouterModule, Routes } from '@angular/router';
import { NgModule } from '@angular/core';

import { KeysListComponent } from './keys-list/keys-list.component';
import { KeysComponent } from './keys.component';
import { KeysDlmsAssociationListComponent } from './keys-dlms-association-list/keys-dlms-association-list.component';

const keysRoutes: Routes = [
  {
    path: '', component: KeysComponent, children: [
      { path: '', component: KeysDlmsAssociationListComponent },
      { path: ':id', component: KeysListComponent }
    ]
  },
];

@NgModule({
  imports: [RouterModule.forChild(keysRoutes)],
  exports: [RouterModule]
})

export class KeysRoutingModule { }
